def ler_seq(FileHandle):
    file=open(FileHandle, "r")
    lines=file.readlines()
    seq=""
    for line in lines:
        seq+=line.replace("\n","")
    file.close()
    return seq

def ler_FASTA_seq(FileHandle):
    file=open(FileHandle, "r")
    fasta_file=file.read()
    fasta_list=fasta_file.split("\n")
    seq_list=fasta_list[1:]
    seq="".join(seq_list)
    return seq
